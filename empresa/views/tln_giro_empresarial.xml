<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="tln_giro_empresarial_view_form" model="ir.ui.view">
			<field name="name">tln_giro_empresarial.form</field>
			<field name="model">tln_giro_empresarial</field>
			<field name="arch" type="xml">
				<form>
					<sheet>
						<group>
							<field name="name" />
							<field name="codigo" />
						</group>
					</sheet>
				</form>
			</field>
		</record>
		<record id="tln_giro_empresarial_view_tree" model="ir.ui.view">
			<field name="name">tln_giro_empresarial.tree</field>
			<field name="model">tln_giro_empresarial</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name" />
					<field name="codigo" />
				</tree>
			</field>
		</record>
		<record id="tln_giro_empresarial_action" model="ir.actions.act_window">
			<field name="name">Giro Empresarial</field>
			<field name="res_model">tln_giro_empresarial</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">
					Nuevo
				</p>
			</field>
		</record>
	</data>
</odoo>
